{
    "name": "sensor_simulator",
    "version": "1.0.0",
    "description": "Simulates sensor data for testing purposes.",
    "author": "Synapse Framework Team",
    "init_function": "sensor_simulator_create",
    "init_level": 10,
    "dependencies": [],
    "type": "testing",
    "build_enabled": true,
    "conditional_config": "CONFIG_MODULE_SENSOR_SIMULATOR_ENABLED",
    "runtime_commands": {
        "start": "sensor_simulator_start",
        "stop": "sensor_simulator_disable",
        "enable": "sensor_simulator_enable",
        "disable": "sensor_simulator_disable",
        "reconfigure": "sensor_simulator_reconfigure",
        "status": "sensor_simulator_get_status",
        "handle_event": "sensor_simulator_handle_event"
    }
}