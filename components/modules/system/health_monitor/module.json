{
    "name": "health_monitor",
    "version": "1.0.0",
    "description": "Monitors system health metrics like heap, CPU, and tasks",
    "author": "Synapse Framework Team",
    "init_function": "health_monitor_create",
    "init_level": 90,
    "dependencies": [],
    "type": "system",
    "build_enabled": true,
    "conditional_config": "CONFIG_MODULE_HEALTH_MONITOR_ENABLED",
    "runtime_commands": {
        "start": "health_monitor_start",
        "stop": "health_monitor_disable",
        "enable": "health_monitor_enable",
        "disable": "health_monitor_disable",
        "reconfigure": "health_monitor_reconfigure",
        "status": "health_monitor_get_status",
        "handle_event": "health_monitor_handle_event"
    }
}