{
    "name": "self_test_manager",
    "version": "1.0.0",
    "description": "Performs system-wide self-diagnostics and health checks.",
    "author": "Synapse Framework Team",
    "init_function": "self_test_manager_create",
    "init_level": 80,
    "dependencies": [],
    "type": "diagnostics",
    "build_enabled": true,
    "conditional_config": "CONFIG_MODULE_SELF_TEST_MANAGER_ENABLED",
    "runtime_commands": {
        "start": "self_test_manager_start",
        "stop": "self_test_manager_disable",
        "enable": "self_test_manager_enable",
        "disable": "self_test_manager_disable",
        "reconfigure": "self_test_manager_reconfigure",
        "status": "self_test_manager_get_status",
        "handle_event": "self_test_manager_handle_event"
    },
    "mqtt_interface": {
    "publishes": {
        "selftest_report": "data/selftest/report"
    },
    "subscribes": {}
}
}